{
  "ConnectionStrings": {
    "AzureStorage": "UseDevelopmentStorage=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ReportBuilder": "Information",
      "ReportBuilder.Service": "Debug",
      "ReportBuilder.Infrastructure.Middleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "Analytics": {
    "MaxConcurrentQueries": 3,
    "MemoryLimitMB": 500,
    "WarningMemoryLimitMB": 400,
    "DefaultTimeoutSeconds": 300,
    "MaxRetries": 3,
    "ChunkSizeBytes": 65536
  },
  "BackpressureSettings": {
    "MaxChannelBufferSize": 10000,
    "ThrottleDelayMs": 10,
    "FlushAfterEachRow": true,
    "EnableThrottling": true,
    "MaxQueueThreshold": 10000
  },
  "JsonStreamingSettings": {
    "MeasureSerializationLatency": true,
    "EnablePropertyNameCaseInsensitive": true,
    "WriteIndented": false,
    "MaxDepth": 64,
    "AllowTrailingCommas": true,
    "DefaultBufferSize": 16384
  }
}
