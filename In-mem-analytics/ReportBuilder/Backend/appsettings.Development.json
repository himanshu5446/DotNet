{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ReportBuilder.Service.BackpressureStreamingService": "Debug",
      "ReportBuilder.BackpressureStreamingController": "Debug"
    }
  },
  "BackpressureSettings": {
    "MaxChannelBufferSize": 1000,
    "ThrottleDelayMs": 5,
    "FlushAfterEachRow": true,
    "EnableThrottling": true,
    "MaxQueueThreshold": 1000
  },
  "JsonStreamingSettings": {
    "MeasureSerializationLatency": true,
    "EnablePropertyNameCaseInsensitive": true,
    "WriteIndented": false,
    "MaxDepth": 32,
    "AllowTrailingCommas": true,
    "DefaultBufferSize": 8192
  }
}
